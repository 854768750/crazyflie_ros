if __name__ == '__main__':
    figure8 = Figure8(
        [
            #x   ,   y,   z, yaw, sleep
            [-0.3 , -1.4, 0.6, 0, 8],
	    [-0.3 , -1.4, 0.6, 0, 3],
	    #[0.7 , -1.4, 1.0, 0, 10],
   	    #[0.7 , -0.4, 1.0, 0, 10],
	    #[-0.3 , -0.4, 1.0, 0, 10],
  	    [-0.3 , -1.4, 0.0, 0, 0],
            #[1.5 , 0.0, 0.5, 0, 2],
            #[-1.5 , 0.0, 0.75, 0, 2],
            #[-1.5 , 0.5, 0.5, 0, 2],
            #[0.0 , 0.0, 0.5, 0, 0],
            #[0.0 , 1.0, 0.3, 0],
            #[1.0 , 1.0, 0.5, 0],
            #[0.0 , 0.5, 0.2, 0],
            #[0.0 , 0.5, 0.0, 0],
        ]
    )
    figure8.run()
